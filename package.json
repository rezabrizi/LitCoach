{
    "name": "litcoach",
    "description": "Provides AI-powered assistance on LeetCode and automatically syncs your successful submissions to GitHub",
    "private": true,
    "version": "0.1.0",
    "type": "module",
    "scripts": {
        "start": "concurrently \"npm run dev\" \"npm run api\"",
        "dev": "vite",
        "api": "uvicorn api.app:app --reload",
        "build": "vite build",
        "lint": "eslint .",
        "format": "prettier --write . --tab-width 4 --print-width 115",
        "preview": "vite preview"
    },
    "dependencies": {
        "@hookform/resolvers": "^3.10.0",
        "@radix-ui/react-alert-dialog": "^1.1.6",
        "@radix-ui/react-avatar": "^1.1.3",
        "@radix-ui/react-dialog": "^1.1.6",
        "@radix-ui/react-dropdown-menu": "^2.1.2",
        "@radix-ui/react-hover-card": "^1.1.6",
        "@radix-ui/react-label": "^2.1.1",
        "@radix-ui/react-popover": "^1.1.6",
        "@radix-ui/react-scroll-area": "^1.2.2",
        "@radix-ui/react-select": "^2.1.5",
        "@radix-ui/react-slot": "^1.1.2",
        "@radix-ui/react-switch": "^1.1.2",
        "@radix-ui/react-tabs": "^1.1.2",
        "@radix-ui/react-toast": "^1.2.2",
        "axios": "^1.7.9",
        "class-variance-authority": "^0.7.0",
        "clsx": "^2.1.1",
        "cmdk": "^1.0.0",
        "lucide-react": "^0.454.0",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-hook-form": "^7.54.2",
        "react-markdown": "^9.0.1",
        "react-syntax-highlighter": "^15.6.1",
        "tailwind-merge": "^2.5.4",
        "tailwindcss-animate": "^1.0.7",
        "zod": "^3.24.1"
    },
    "devDependencies": {
        "@crxjs/vite-plugin": "^2.0.0-beta.25",
        "@eslint/js": "^9.14.0",
        "@tailwindcss/typography": "^0.5.15",
        "@types/react": "^18.3.12",
        "@types/react-dom": "^18.3.1",
        "@vitejs/plugin-react": "^4.3.3",
        "autoprefixer": "^10.4.20",
        "concurrently": "^9.1.2",
        "eslint": "^9.14.0",
        "eslint-plugin-react": "^7.37.2",
        "eslint-plugin-react-hooks": "^5.1.0-rc.0",
        "eslint-plugin-react-refresh": "^0.4.14",
