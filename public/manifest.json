{
    "manifest_version": 3,
    "name": "LitCoach",
    "permissions": ["sidePanel", "tabs", "scripting", "activeTab"],
    "icons": {
        "16": "icon16.png",
        "32": "icon32.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "side_panel": {
        "default_path": "index.html"
    },
    "background": {
        "service_worker": "src/background.js",
        "type": "module"
    },
    "host_permissions": ["https://leetcode.com/*"],
    "key": ""
}
